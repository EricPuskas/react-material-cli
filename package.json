{
  "name": "react-material-cli",
  "version": "0.1.2",
  "description": "Generate custom react components and hooks that follow best practices and the highest standards of implementation.",
  "main": "index.js",
  "bin": {
    "rmc": "dist/index.js"
  },
  "files": [
    "dist",
    "templates"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "npm run prepublishOnly",
    "prepublishOnly": "esbuild src/index.ts --platform=node --outfile=dist/index.js --bundle --minify"
  },
  "author": "Eric Puskas",
  "license": "MIT",
  "keywords": [
    "react",
    "reactjs",
    "cli",
    "components",
    "hooks"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/EricPuskas/react-material-cli"
  },
  "devDependencies": {
    "@types/node": "^16.0.0",
    "chalk": "^4.1.1",
    "commander": "^8.0.0",
    "esbuild": "^0.12.15",
    "typescript": "^4.3.5"
  }
}
